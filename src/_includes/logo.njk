{% macro m(fill = "hsl(0,0%,100%)") %}

  <svg class="mm-logo" fill="{{fill}}" preserveAspectRatio="xMidYMid meet" viewBox="0 0 109 59.5" width="32px" height="32px">
    <polygon points="0,0 0,59.5 11,59.5 11,24 30.5,43.9 50,24 50,59.5 60,59.5 60,0 30,29.8 "/>
    <polygon points="79.4,29.9 66,16.2 66,30.3 79.8,44 100,24 100,59.5 109,59.5 109,0 "/>
    <title>Mark Michon</title>
  </svg>
{% endmacro %}

{% macro createIcon(width = "24px", path) %}
  <svg
    class="icon"
    preserveAspectRatio="xMidYMid meet"
    width="{{width}}"
    height="{{width}}"
  >
    <path d="{{path}}"/>
  </svg>
{% endmacro %}